<div class="card-container">
  <div class="explore-search-wrapper">
    <div class="index-explore">Musicians</div>
  </div>

  <div class="musician-container">
    <div class="row">
      <% @musicians.each do |musician| %>
        <div class="col-xs-12 col-md-4">
          <div class="musician-index">
            <div class="musician-card">
              <%= link_to musician_path(musician) do %>
                <%= cl_image_tag(musician.user_photo, class: 'musician-image') %>
                <div>
                  <img class="instrument-icon" src="https://res.cloudinary.com/harrisongarovi/image/upload/v1552555252/guitar-64.png">
                  <img class="instrument-icon2" src="https://res.cloudinary.com/harrisongarovi/image/upload/v1552555509/side-drum-64.png">
                  <img class ="instrument-icon3" src="https://res.cloudinary.com/harrisongarovi/image/upload/v1552556196/piano.png">
                  <img class="instrument-icons4" src="https://res.cloudinary.com/harrisongarovi/image/upload/v1552556495/microphone-9-64.png">
                </div>
                <div class="musician-description-shadow">
                <div class="musician-card-description">
                  <div style="display: flex">
                <p class="nickname"><%= musician.nickname.capitalize! %></p>

                <div class="musician-equipment">
                   <% instruments = musician.equipments.map do |e| %>
                      <p><%= e.equipment_type.capitalize  %></p>
                    <% end %>
                </div>
                <p class="tag-line"><%= musician.tag_line %></p>
                <p class="location"><i class="fas fa-map-marked-alt"></i> <%= musician.location.capitalize! %></p>

              </div>
              </div>

              <% end %>
            </div>
          </div>
        </div>
      <% end %>
    </div>
  </div>
</div>
